{
  "name": "dappstore-subgraph",
  "version": "1.0.0",
  "description": "The Graph subgraph for DApp Store",
  "scripts": {
    "codegen": "graph codegen",
    "codegen:sepolia": "graph codegen subgraph.sepolia.yaml",
    "build": "graph build",
    "build:sepolia": "graph build subgraph.sepolia.yaml",
    "auth": "graph auth --studio",
    "deploy:studio": "graph deploy --studio dappstore",
    "deploy:sepolia": "graph deploy --studio dappstore subgraph.sepolia.yaml",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 dappstore",
    "local:full": "npm run codegen && npm run build && npm run create-local && npm run deploy-local",
    "create-local": "graph create --node http://localhost:8020/ dappstore",
    "remove-local": "graph remove --node http://localhost:8020/ dappstore",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.56.0",
    "@graphprotocol/graph-ts": "^0.31.0"
  },
  "devDependencies": {
    "matchstick-as": "^0.5.0"
  }
}
